<!DOCTYPE html>
  <head>
    <link rel="stylesheet" href="../style.css">
    <link rel="stylesheet" href="../fonts.css">
  </head>
  <body>
    <div class="header no-border-top no-border-bottom">
    <div class="left">
    <img src="../trademark.pdf" id="header-trademark" onclick="location.href='../index.html'" style="cursor:pointer">
    <h1> {{ title .Name }} </h1>
    </div>
      <div class="center">
      </div>
      <div class="right">
      </div>
    </div>
    <div class="subheader">
      {{ if .Needs }}
        <button disabled class="need label"> Needs: </button>
        {{ range $k, $v := .Needs }}
            <button class="need" tabindex="0" onclick="location.href='./{{ $v }}.html'">
              {{ title $v }}
            </button>
        {{ end }}
      {{ else }}
        <button disabled class="need label"> No needs. </button>
      {{ end }}
      {{ if .NeededBy }}
        <button disabled class="need label"> Needed by: </button>
        {{ range $k, $v := .NeededBy }}
          <button class="need" tabindex="0" onclick="location.href='./{{ $v }}.html'">
            {{ title $v }}
          </button>
        {{ end }}
      {{ end }}
      <button disabled class="need label"> Links: </button>
      <button class="need" tabindex="0" onclick="location.href='./{{ .Name }}.pdf'">
        Sheet PDF
      </button>
      <button class="need" tabindex="0" onclick="location.href='./{{ .Name }}_graph.pdf'">
        Graph PDF
      </button>
    </div>

    <iframe id="sheet" src="./{{ .Name }}.pdf">
    </iframe>

    <iframe id="graph" src="./{{ .Name }}_graph.pdf">
    </iframe>

  </body>
</html>`
