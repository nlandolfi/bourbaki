<!DOCTYPE html>
  <meta charset="utf-8"/>
  <head>
    <link rel="stylesheet" href="../style.css">
    <link rel="stylesheet" href="../fonts.css">
  </head>
  <body>
    <div class="header">
      <span class="listing no-border" id="header-trademark-and-title">
        <img id="header-trademark" src="../trademark.jpg" onclick="location.href='../index.html'">
        <span id="header-title" onclick="location.href='./{{ .Name }}.pdf'"> {{ title .Name }} </span>
      </span>
    </div>
    <div class="subheader">
      <div disabled class="need label"> Links: </div>
      <div class="need" tabindex="0" onclick="location.href='./{{ .Name }}.pdf'">
        <span> Sheet PDF </span>
        <span class="caret">›</span>
      </div>
      <div class="need" tabindex="0" onclick="location.href='./{{ .Name }}-graph.pdf'">
        <span> Graph PDF </span>
        <span class="caret">›</span>
      </div>
      {{ if .Needs }}
        <div disabled class="need label"> Needs: </div>
        {{ range $k, $v := .Needs }}
            <div class="need" tabindex="0" onclick="location.href='./{{ $v }}.html'">
              <span>
                {{ title $v }}
              </span>
              <span class="caret">›</span>
            </div>
        {{ end }}
      {{ else }}
        <div disabled class="need label"> No needs. </div>
      {{ end }}
      {{ if .NeededBy }}
        <div disabled class="need label"> Needed by: </div>
        {{ range $k, $v := .NeededBy }}
          <div class="need" tabindex="0" onclick="location.href='./{{ $v }}.html'">
            <span>
            {{ title $v }}
            </span>
            <span class="caret">›</span>
          </div>
        {{ end }}
      {{ end }}
    </div>

    <iframe id="sheet-pdf" src="./{{ .Name }}.pdf">
    </iframe>

    <iframe id="graph-pdf" src="./{{ .Name }}-graph.pdf">
    </iframe>

  </body>
</html>`
